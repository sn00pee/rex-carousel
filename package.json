{
  "name": "@rakuten-rex/rex-carousel",
  "version": "2.4.7",
  "description": "ReX React UI Component",
  "main": "src/Carousel.jsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init:component": "node init.js && npm i && npm start",
    "start": "NODE_ENV=development ./node_modules/.bin/start-storybook -p 8080 --ci",
    "build": "npm run build:component && npm run build:storybook",
    "build:patch": "npm --no-git-tag-version version patch && npm run build:commit",
    "build:minor": "npm --no-git-tag-version version minor && npm run build:commit",
    "build:major": "npm --no-git-tag-version version major && npm run build:commit",
    "build:storybook": "NODE_ENV=production ./node_modules/.bin/build-storybook -c .storybook -o docs",
    "build:component": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production --config webpack.config.js",
    "build:commit": "npm run build && npm run commit",
    "commit": "npm run commit:docs && npm run commit:package",
    "commit:docs": "git add docs README.md && git commit -m 'Added - Docs for github pages and README file'",
    "commit:package": "git add package.json package-lock.json && git commit -m 'New version - v'$npm_package_version",
    "serve": "NODE_ENV=production ./node_modules/.bin/http-server -p 8081 docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakuten-rex/rex-carousel.git"
  },
  "author": "ReX React UI Components Library Authors (https://github.com/rakuten-rex/rex-carousel/graphs/contributors)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rakuten-rex/rex-carousel/issues"
  },
  "homepage": "https://rakuten-rex.github.io/rex-carousel/",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-id-swiper": "^2.3.1",
    "rex-core": "^1.4.0",
    "rex-icons": "^1.2.1",
    "rex-react-utils": "^1.0.1",
    "swiper": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@rakuten-rex/eslint-config-rex-react-component": "^1.3.2",
    "@rakuten-rex/storybook-inspecthtml": "^1.0.3",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-google-analytics": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "google-closure-compiler-js": "^20180610.0.0",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "storybook-inspecthtml": "0.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1"
  }
}
